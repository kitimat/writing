{
  "name": "journal",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Gabe Scholz",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^7.0.10",
    "@types/react": "^16.9.9",
    "@types/react-dom": "^16.9.2",
    "autoprefixer": "^9.6.5",
    "husky": "^3.0.9",
    "parcel-bundler": "^1.12.4",
    "postcss-modules": "^1.4.1",
    "preact": "^10.0.0",
    "typescript": "^3.6.4"
  },
  "scripts": {
    "dev": "parcel src/index.html --out-dir .build",
    "build": "rm -rf build && parcel build src/index.html --out-dir build"
  },
  "alias": {
    "react": "preact/compat",
    "react-dom": "preact/compat"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn build && git add build"
    }
  }
}
